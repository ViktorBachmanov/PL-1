  "main": "index.js",
  "scripts": {
    "scss": "node-sass --output-style compressed -o dist/css src/scss",
    "serve": "browser-sync start --server --open false --files \"dist/css/*.css, **/*.html\"",
    "watch:css": "node-sass --watch src/scss -o dist/css",
    "start": "run-p serve watch:css"
  },
